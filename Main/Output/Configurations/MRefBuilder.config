<!-- This is configuration file for the MRefBuilder tool. -->
<configuration>
    <dduetools>
        <!-- 1. Specify the platform for the reflection. -->
        <SandcastleItem name="referencePlatform"/>
        <!-- 2. Specify the assembly resolver for the reflection. -->
        <SandcastleItem name="referenceResolver"/>
        <!-- 3. Specify the API namer for the reflection. -->
        <SandcastleItem name="referenceNamer"/>
        <!-- 4. Specify any addin - must be an implementation of MRefBuilderAddIn. -->
        <SandcastleItem name="referenceAddins"/>
        <!-- 5. Specify reflection options. -->
        <SandcastleItem name="referenceOptions"/>
        <!-- 6. Specify the default and user attribute filters -->
        <SandcastleItem name="referenceAttributeFilter"/>
        <!-- 7. Specify the API or type filter. -->
        <SandcastleItem name="referenceApiFilter"/>
    </dduetools>
</configuration>
